# Location of your dotfiles in $HOME
# DOTFILES=.dotfiles

# dotfiles to symlink
#
# srcfile:dstfile
# dstfile will be '.$srcfile' by default (with path removed)
#
[files]
dotsyncrc
curl/curlrc
git/gitconfig
git/gitignore
git/gitattributes
input/inputrc
tmux/tmux.conf
wget/wgetrc
bash/bashrc
bash/bash_profile
bash/bash_logout
bash/exports
bash/functions
bash/logout
bash/bash_aliases
lein/lein:bin/lein
lein/profiles.clj:.lein/profiles.clj
emacs.d
osx/install-brew.sh:install-brew.sh
osx/amethyst
z:z
tmuxifier
scripts/ec:bin/ec
scripts/es:bin/es
scripts/et:bin/et
scripts/ed.el:bin/ed.el
scripts/offlineimap.py:.offlineimap.py
fasd:fasd
fasd/fasd:bin/fasd
slate/slate.js
ag/agignore
hammerspoon
osx/org.gnupg.gpg-agent.plist:/Library/LaunchAgents/org.gnupg.gpg-agent.plist
osx/startup-gpg-agent.sh:bin/startup-gpg-agent.sh
osx/gpg-agent.conf:/.gnupg/gpg-agent.conf
qfc
liquidprompt
prompt/liquidpromptrc:.config/liquidpromptrc
kwm/kwmrc:.kwm/kwmrc
kwm/khdrc:.khdrc
hs-weather:.hammerspoon/hs-weather

[endfiles]

## hostname                     type=masterhost, ANY or NONE    files=dotfile:tolink,another:tolink
[hosts]
#host1.example.com               git=ANY                        files=custom/dotfile    #git repo, pull from anywhere and link a custom dotfile
#host2.example.com               git=ANY                                                #git repo, pull from anywhere
#host3.example.com               git=ANY                                                #git repo, pull from anywhere
#host4.example.com               git=host5.example.com                                  #git repo, pull from host5
#host5.example.com               git=NONE                                               #git repo, local only
#host6.example.net               rsync=host1.example.com                                #rsync mirror, push from host1
#host7.example.net               rsync=host2.example.com                                #rsync mirror, push from host2
#host8.example.net               rsync=host2.example.com                                #rsync mirror, push from host2
#host9.example.net               rsync=host2.example.com                                #rsync mirror, push from host2
hanuman	git=ANY
precise32 git=ANY
[endhosts]
